{
  "env": {
    "production": {
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-inline-elements"
      ]
    },
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        "transform-react-jsx-self"
      ]
    }
  },
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@internal/colors": "./source/@internal/colors",
          "@internal/decorators": "./source/@internal/decorators",
          "@internal/elements": "./source/@internal/elements",
          "@internal/pages": "./source/@internal/pages",
          "@internal/queries": "./source/@internal/queries",
          "@internal/reactions": "./source/@internal/reactions",
          "@internal/resources": "./source/@internal/resources",
          "@internal/route": "./source/@internal/route",
          "@internal/signals": "./source/@internal/signals",
          "@internal/styles": "./source/@internal/styles",
          "@internal/ui": "./source/@internal/ui"
        }
      }
    ],
    "pipe-operator-curry",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-runtime",
    "external-helpers"
  ]
}
