{
  "name": "@lacqueristas/www",
  "version": "1.1.0",
  "description": "The frontend browser client for the lacqueristas API",
  "scripts": {
    "start": "node './transpiled/server/index.js'",
    "build": "gulp build",
    "build:server": "gulp server",
    "build:client": "gulp client",
    "watch": "npm-run-all --parallel watch:build watch:lint watch:test",
    "watch:lint": "chokidar './source/' --ignore '/node_modules/' --command 'npm run lint --silent'",
    "watch:test": "chokidar './source/' --ignore '/node_modules/' --command 'npm run test --silent'",
    "watch:build": "gulp watch",
    "test": "tap './source/**/test.js'",
    "check": "flow './source'",
    "lint": "eslint './source/' --ignore-pattern 'node_modules'"
  },
  "keywords": [
    "lacqueristas",
    "nailpolish",
    "frontend"
  ],
  "author": "Kurtis Rainbolt-Greene (@krainboltgreene) <me@kurtisrainboltgreene.name>",
  "license": "MIT",
  "homepage": "https://github.com/lacqueristas/www#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lacqueristas/www.git"
  },
  "bugs": {
    "url": "https://github.com/lacqueristas/www/issues"
  },
