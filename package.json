{
  "name": "www",
  "version": "1.0.0",
  "description": "The frontend browser client for the lacqueristas API",
  "main": "tmp/client/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "npm-run-all --parallel watch server",
    "build": "npm-run-all build:client build:static",
    "build:client": "mkdir -p './tmp/client' && browserify --transform babelify --transform envify --entry './client/index.js' --outfile './tmp/client/index.js'",
    "build:static": "gulp build",
    "lint": "npm-run-all --parallel lint:client lint:test",
    "lint:client": "eslint ./client/",
    "lint:static": "eslint ./static/",
    "lint:test": "eslint './test/'",
    "test": "mocha --compilers js:babel-register ./test/**/test.js",
    "watch": "npm-run-all --parallel watch:build watch:lint watch:test",
    "watch:build": "npm-run-all --parallel watch:build:client watch:build:static",
    "watch:build:client": "mkdir -p './tmp/client' && watchify --verbose --transform babelify --transform envify --entry './client/index.js' --outfile './tmp/client/index.js'",
    "watch:build:static": "gulp watch",
    "watch:lint": "npm-run-all --parallel watch:lint:client watch:lint:test",
    "watch:lint:client": "chokidar './client/' --command 'npm run lint:client'",
    "watch:lint:static": "chokidar './static/' --command 'npm run lint:static'",
    "watch:lint:test": "chokidar './test/' --command 'npm run lint:test'",
    "watch:test": "chokidar './test/' './client/' './static/' --command 'npm run test'",
    "server": "npm run server:`echo $NODE_ENV`",
    "server:development": "http-server './tmp/static'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lacqueristas/www.git"
  },
  "keywords": [
    "lacqueristas",
    "nailpolish",
    "frontend"
  ],
  "author": "Kurtis Rainbolt-Greene (@krainboltgreene) <me@kurtisrainboltgreene.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lacqueristas/www/issues"
  },
  "homepage": "https://github.com/lacqueristas/www#readme",
  "engines": {
    "node": "5.5.0",
    "npm": "3.5.3"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.6.5",
    "babel-core": "6.6.5",
    "babel-eslint": "5.0.0",
    "babel-plugin-syntax-flow": "6.5.0",
    "babel-plugin-transform-async-to-generator": "6.5.0",
    "babel-plugin-transform-async-to-module-method": "6.5.2",
    "babel-plugin-transform-class-constructor-call": "6.6.5",
    "babel-plugin-transform-class-properties": "6.6.0",
    "babel-plugin-transform-decorators": "6.6.5",
    "babel-plugin-transform-do-expressions": "6.5.0",
    "babel-plugin-transform-es2015-arrow-functions": "6.5.2",
    "babel-plugin-transform-es2015-block-scoped-functions": "6.6.5",
    "babel-plugin-transform-es2015-block-scoping": "6.6.5",
    "babel-plugin-transform-es2015-classes": "6.6.5",
    "babel-plugin-transform-es2015-computed-properties": "6.6.5",
    "babel-plugin-transform-es2015-destructuring": "6.6.5",
