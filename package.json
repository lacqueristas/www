{
  "name": "@lacqueristas/www",
  "version": "1.0.0",
  "description": "The frontend browser client for the lacqueristas API",
  "main": "transpiled/server/index.js",
  "scripts": {
    "start": "nodemon --watch './transpiled/server' './transpiled/server/index.js'",
    "now-build": "",
    "deploy": "npm-run-all deploy:build deploy:ship",
    "deploy:build": "NODE_ENV=production npm run build",
    "deploy:ship": "now deploy --env NODE_ENV=production --env PORT=8080",
    "build": "npm-run-all build:transpiled --parallel build:server build:assets build:client",
    "build:transpiled": "rm -rf './transpiled/' && mkdir -p './transpiled/client/' './transpiled/server'",
    "build:server": "babel --ignore './source/server/**/test.js' -d './transpiled/server/' './source/server/'",
    "build:client": "browserify --verbose --transform babelify --entry './source/client/index.js' --outfile './transpiled/client/index.js'",
    "build:assets": "gulp build",
    "watch": "npm-run-all --parallel watch:build watch:lint watch:test",
    "watch:lint": "chokidar './source/' --command 'npm run lint --silent'",
    "watch:test": "mocha --watch --compilers js:babel-register './source/**/test.js'",
    "watch:build": "npm-run-all --parallel watch:build:server watch:build:client watch:build:assets",
    "watch:build:server": "babel --watch --retain-lines --ignore './source/server/**/test.js' -d './transpiled/server/' './source/server/'",
    "watch:build:client": "watchify --verbose --transform babelify --entry './source/client/index.js' --outfile './transpiled/client/index.js'",
    "watch:build:assets": "gulp watch",
    "test": "mocha --compilers js:babel-register './source/**/test.js'",
    "lint": "eslint './source/'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lacqueristas/www.git"
  },
  "keywords": [
    "lacqueristas",
    "nailpolish",
    "frontend"
  ],
  "author": "Kurtis Rainbolt-Greene (@krainboltgreene) <me@kurtisrainboltgreene.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lacqueristas/www/issues"
  },
  "homepage": "https://github.com/lacqueristas/www#readme",
  "engines": {
    "node": "^6",
    "npm": "^3"
  },
  "devDependencies": {
    "babel": "^6",
    "babel-cli": "^6",
    "babel-eslint": "^7",
    "babel-plugin-relative-import": "^1",
    "babel-plugin-transform-class-properties": "^6",
    "babel-plugin-transform-object-rest-spread": "^6",
    "babel-plugin-transform-runtime": "^6",
    "babel-preset-latest": "^6",
    "babel-preset-react": "^6",
    "babelify": "^7",
    "browserify": "^13",
    "chai": "^3",
    "chokidar-cli": "^1",
    "eslint": "^3",
    "eslint-plugin-babel": "^3",
    "eslint-plugin-immutable": "^1",
    "eslint-plugin-jsx-a11y": "^2",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-mocha": "^4",
    "eslint-plugin-node": "^3",
    "eslint-plugin-react": "^6",
    "greenkeeper": "^4",
    "gulp": "^3",
    "gulp-cli": "^1",
    "gulp-concat": "^2",
    "gulp-gzip": "^1",
    "gulp-image": "^2",
    "gulp-myth": "^1",
    "gulp-size": "^2",
    "mocha": "^3",
    "now": "0.27.0",
    "font-awesome": "^4",
    "normalize.css": "^5",
    "watchify": "^3"
  },
  "dependencies": {
    "abstraction": "^8",
    "body-parser": "^1",
    "bunyan": "^1",
    "case": "^1",
    "compression": "^1",
    "cors": "^2",
    "dotenv": "^2",
    "express": "^4",
    "history": "^4",
    "httpstatuses": "^3",
    "morgan": "^1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3",
    "pusher-js": "^3",
    "ramda": "0.22.1",
    "ramda-extra": "^1",
    "raw-body": "^2",
    "react": "^15",
    "react-addons-shallow-compare": "^15",
    "react-dom": "^15",
    "react-logger": "^1",
    "react-redux": "^4",
    "redux": "^3",
    "redux-logger": "^2",
    "regexp-map": "0.3.1",
    "require-environment-variables": "^1",
    "url-parse": "^1"
  }
}
