{
  "name": "www",
  "version": "1.0.0",
  "description": "The frontend browser client for the lacqueristas API",
  "main": "tmp/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "test": "mocha --compilers js:babel-register client/**/test.js",
    "lint": "eslint './client/'",
    "start": "npm run start:`echo $NODE_ENV`",
    "start:development": "http-server -c-1 './tmp/'",
    "start:production": "http-server -p $PORT './tmp/'",
    "build": "mkdir -p './tmp/' && npm-run-all build:client build:assets",
    "build:client": "browserify --transform babelify --entry './client/index.js' --outfile './tmp/index.js'",
    "build:assets": "gulp build",
    "watch": "npm-run-all --parallel watch:build watch:lint watch:test start",
    "watch:build": "mkdir -p './tmp/' && npm-run-all --parallel watch:build:client watch:build:assets",
    "watch:build:client": "watchify --plugin browserify-hmr --transform babelify --debug --verbose --entry './client/index.js' --outfile './tmp/index.js'",
    "watch:build:assets": "gulp watch",
    "watch:lint": "chokidar './client/' --initial --command 'npm run lint --silent'",
    "watch:test": "chokidar './client/' --initial --command 'npm run test --silent'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lacqueristas/www.git"
  },
  "keywords": [
    "lacqueristas",
    "nailpolish",
    "frontend"
  ],
  "author": "Kurtis Rainbolt-Greene (@krainboltgreene) <me@kurtisrainboltgreene.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lacqueristas/www/issues"
  },
  "homepage": "https://github.com/lacqueristas/www#readme",
  "engines": {
    "node": "5.10.0",
    "npm": "3.8.3"
  },
  "devDependencies": {
    "babel-eslint": "6.0.3",
    "chai": "3.5.0",
    "chokidar-cli": "1.2.0",
    "eslint": "2.8.0",
    "isparta": "4.0.0",
    "mocha": "2.4.5",
    "npm-check-updates": "2.6.3",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "@cycle/core": "6.0.3",
    "@cycle/http": "8.2.2",
    "babel-plugin-transform-es2015-arrow-functions": "6.7.7",
    "babel-plugin-transform-es2015-computed-properties": "6.6.5",
    "babel-plugin-transform-es2015-destructuring": "6.6.5",
    "babel-plugin-transform-es2015-function-name": "6.5.0",
    "babel-plugin-transform-es2015-literals": "6.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.7.7",
    "babel-plugin-transform-es2015-parameters": "6.7.0",
    "babel-plugin-transform-es2015-shorthand-properties": "6.5.0",
    "babel-plugin-transform-es2015-spread": "6.6.5",
    "babel-plugin-transform-es2015-sticky-regex": "6.5.0",
    "babel-plugin-transform-es2015-typeof-symbol": "6.6.0",
    "babel-plugin-transform-es2015-unicode-regex": "6.5.0",
    "babel-plugin-transform-es3-member-expression-literals": "6.5.0",
    "babel-plugin-transform-es3-property-literals": "6.5.0",
    "babel-plugin-transform-es5-property-mutators": "6.6.5",
    "babel-plugin-transform-eval": "6.5.0",
    "babel-plugin-transform-exponentiation-operator": "6.5.0",
    "babel-plugin-transform-export-extensions": "6.5.0",
    "babel-plugin-transform-function-bind": "6.5.2",
    "babel-plugin-transform-member-expression-literals": "6.5.0",
    "babel-plugin-transform-object-assign": "6.5.0",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-plugin-transform-object-set-prototype-of-to-assign": "6.6.0",
    "babel-plugin-transform-property-literals": "6.5.0",
    "babel-plugin-transform-proto-to-assign": "6.6.5",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-root-import": "3.2.2",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "browserify-hmr": "0.3.1",
    "cycle-restart": "0.0.14",
    "cycle-snabbdom": "1.2.1",
    "dotenv": "^2.0.0",
    "eslint-plugin-babel": "3.2.0",
    "gulp": "3.9.1",
    "gulp-cli": "1.2.1",
    "gulp-concat": "2.6.0",
    "gulp-gzip": "1.2.0",
    "gulp-image": "1.3.1",
    "gulp-mustache": "^2.2.0",
    "gulp-myth": "1.1.0",
    "gulp-size": "2.1.0",
    "http-server": "0.9.0",
    "mustache": "2.2.1",
    "normalize.css": "4.1.1",
    "npm-run-all": "1.7.0",
    "ramda": "0.21.0",
    "rx": "4.1.0",
    "snabbdom-helpers": "3.0.0",
    "uglifyify": "3.0.1"
  }
}
