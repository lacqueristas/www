{
  "name": "www",
  "version": "1.0.0",
  "description": "The frontend browser client for the lacqueristas API",
  "main": "tmp/client/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "npm-run-all --parallel watch server",
    "test": "mocha --compilers js:babel-register client/**/test.js",
    "lint": "eslint './client/'",
    "server": "npm run server:`echo $NODE_ENV`",
    "server:development": "http-server -c-1 './tmp/'",
    "server:production": "http-server -p $PORT './tmp/'",
    "build": "mkdir -p './tmp/' && npm-run-all build:client build:assets",
    "build:client": "browserify --transform babelify --transform uglifyify --entry './client/index.js' --outfile './tmp/index.js'",
    "build:assets": "gulp build",
    "watch": "npm-run-all --parallel watch:build watch:lint watch:test",
    "watch:build": "mkdir -p './tmp/' && npm-run-all --parallel watch:build:client watch:build:assets",
    "watch:build:client": "watchify --transform babelify --debug --verbose --entry './client/index.js' --outfile './tmp/index.js'",
    "watch:build:assets": "gulp watch",
    "watch:lint": "chokidar './client/' --initial --command 'npm run lint --silent'",
    "watch:test": "chokidar './client/' --initial --command 'npm run test --silent'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lacqueristas/www.git"
  },
  "keywords": [
    "lacqueristas",
    "nailpolish",
    "frontend"
  ],
  "author": "Kurtis Rainbolt-Greene (@krainboltgreene) <me@kurtisrainboltgreene.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lacqueristas/www/issues"
  },
  "homepage": "https://github.com/lacqueristas/www#readme",
  "engines": {
    "node": "5.5.0",
    "npm": "3.5.3"
  },
  "devDependencies": {
    "babel-eslint": "6.0.2",
    "chai": "3.5.0",
    "chokidar-cli": "1.2.0",
    "eslint": "2.6.0",
    "eslint-plugin-babel": "3.1.0",
    "estraverse-fb": "1.3.1",
    "flow-bin": "0.22.1",
    "isparta": "4.0.0",
    "mocha": "2.4.5",
    "npm-check-updates": "2.6.1",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "@cycle/core": "6.0.3",
    "@cycle/dom": "9.2.2",
