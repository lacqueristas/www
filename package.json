{
  "name": "www",
  "version": "1.0.0",
  "description": "The frontend browser client for the lacqueristas API",
  "main": "tmp/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "test": "mocha --compilers js:babel-register client/**/test.js",
    "lint": "eslint './client/'",
    "start": "npm run start:`echo $NODE_ENV`",
    "start:development": "http-server -c-1 './tmp/'",
    "start:production": "http-server -p $PORT './tmp/'",
    "build": "mkdir -p './tmp/' && npm-run-all build:client build:assets",
    "build:client": "browserify --transform babelify --entry './client/index.js' --outfile './tmp/index.js'",
    "build:assets": "gulp build",
    "watch": "npm-run-all --parallel watch:build watch:lint watch:test start",
    "watch:build": "mkdir -p './tmp/' && npm-run-all --parallel watch:build:client watch:build:assets",
    "watch:build:client": "watchify --transform babelify --debug --verbose --entry './client/index.js' --outfile './tmp/index.js'",
    "watch:build:assets": "gulp watch",
    "watch:lint": "chokidar './client/' --initial --command 'npm run lint --silent'",
    "watch:test": "chokidar './client/' --initial --command 'npm run test --silent'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lacqueristas/www.git"
  },
  "keywords": [
    "lacqueristas",
    "nailpolish",
    "frontend"
  ],
  "author": "Kurtis Rainbolt-Greene (@krainboltgreene) <me@kurtisrainboltgreene.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lacqueristas/www/issues"
  },
  "homepage": "https://github.com/lacqueristas/www#readme",
  "engines": {
    "node": "^6",
    "npm": "^3"
  },
  "devDependencies": {
    "babel-eslint": "^6",
    "chai": "^3",
    "chokidar-cli": "^1",
    "eslint": "^3.2.1",
    "eslint-plugin-babel": "^3",
    "eslint-plugin-immutable": "^1",
    "eslint-plugin-import": "^1",
    "eslint-plugin-mocha": "^2",
    "eslint-plugin-node": "^1",
    "isparta": "^4",
    "mocha": "^2",
    "watchify": "^3"
  },
  "dependencies": {
    "@cycle/core": "^6",
    "@cycle/http": "^8",
    "babel-plugin-transform-es2015-arrow-functions": "^6",
    "babel-plugin-transform-es2015-computed-properties": "^6",
    "babel-plugin-transform-es2015-destructuring": "^6",
    "babel-plugin-transform-es2015-function-name": "^6",
    "babel-plugin-transform-es2015-literals": "^6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6",
    "babel-plugin-transform-es2015-parameters": "^6",
    "babel-plugin-transform-es2015-shorthand-properties": "^6",
    "babel-plugin-transform-es2015-spread": "^6",
    "babel-plugin-transform-es2015-sticky-regex": "^6",
    "babel-plugin-transform-es2015-typeof-symbol": "^6",
    "babel-plugin-transform-es2015-unicode-regex": "^6",
    "babel-plugin-transform-es3-member-expression-literals": "^6",
    "babel-plugin-transform-es3-property-literals": "^6",
    "babel-plugin-transform-es5-property-mutators": "^6",
    "babel-plugin-transform-eval": "^6",
    "babel-plugin-transform-exponentiation-operator": "^6",
    "babel-plugin-transform-export-extensions": "^6",
    "babel-plugin-transform-function-bind": "^6",
    "babel-plugin-transform-member-expression-literals": "^6",
    "babel-plugin-transform-object-assign": "^6",
    "babel-plugin-transform-object-rest-spread": "^6",
    "babel-plugin-transform-object-set-prototype-of-to-assign": "^6",
    "babel-plugin-transform-property-literals": "^6",
    "babel-plugin-transform-proto-to-assign": "^6",
    "babel-plugin-transform-runtime": "^6",
    "babel-root-import": "^3",
    "babelify": "^7",
    "browserify": "^13",
    "cycle-snabbdom": "^1",
    "cycle-router": "^3",
    "dotenv": "^2",
    "eslint-plugin-babel": "^3",
    "gulp": "^3",
    "gulp-cli": "^1",
    "gulp-concat": "^2",
    "gulp-gzip": "^1",
    "gulp-image": "^2.0.0",
    "gulp-mustache": "^2",
    "gulp-myth": "^1",
    "gulp-size": "^2",
    "http-server": "0.9.0",
    "mustache": "^2",
    "normalize.css": "^4",
    "npm-run-all": "^2.1.1",
    "ramda": "0.21.0",
    "rx": "^4",
    "snabbdom-helpers": "^3",
    "uglifyify": "^3"
  }
}
