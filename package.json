{
  "name": "@lacqueristas/www",
  "version": "1.1.0",
  "description": "The frontend browser client for the lacqueristas API",
  "scripts": {
    "start": "babel-node './transpiled/server/index.js'",
    "link": "linklocal link --long --recursive",
    "build": "gulp build",
    "build:server": "gulp server",
    "build:client": "gulp client",
    "watch": "npm-run-all --parallel watch:build watch:lint watch:test",
    "watch:lint": "chokidar './source/' --ignore '/node_modules/' --command 'npm run lint --silent'",
    "watch:test": "chokidar './source/' --ignore '/node_modules/' --command 'npm run test --silent'",
    "watch:build": "gulp watch",
    "test": "mocha --compilers js:babel-register './source/**/test.js'",
    "lint": "eslint './source/' --ignore-pattern 'node_modules'"
  },
  "keywords": [
    "lacqueristas",
    "nailpolish",
    "frontend"
  ],
  "author": "Kurtis Rainbolt-Greene (@krainboltgreene) <me@kurtisrainboltgreene.name>",
  "license": "MIT",
  "homepage": "https://github.com/lacqueristas/www#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lacqueristas/www.git"
  },
  "bugs": {
    "url": "https://github.com/lacqueristas/www/issues"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "chokidar-cli": "1.2.0",
    "eslint": "3.16.1",
    "eslint-plugin-babel": "4.1.0",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-immutable": "1.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "6.10.0",
    "flow": "0.2.3",
    "flow-bin": "0.40.0",
    "flow-typed": "2.0.0",
    "greenkeeper": "4.2.1",
    "npm-check-updates": "2.10.3"
  },
  "dependencies": {
    "@lacqueristas/colors": "file:./source/@lacqueristas/colors",
