{
  "name": "www",
  "version": "1.0.0",
  "description": "The frontend browser client for the lacqueristas API",
  "main": "client/index.js",
  "scripts": {
    "start": "npm-run-all --parallel build server test",
    "build": "npm-run-all --parallel build:client build:static",
    "build:client": "mkdir -p ./tmp/client && watchify --transform babelify --transform envify --entry ./client/index.js --outfile ./tmp/client/index.js",
    "build:static": "gulp",
    "server": "npm run server:`echo $NODE_ENV`",
    "server:development": "http-server ./tmp/static",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lacqueristas/www.git"
  },
  "keywords": [
    "lacqueristas",
    "nailpolish",
    "frontend"
  ],
  "author": "Kurtis Rainbolt-Greene (@krainboltgreene) <me@kurtisrainboltgreene.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lacqueristas/www/issues"
  },
  "homepage": "https://lacqueristas.github.io/www",
  "engines": {
    "node": "5.5.0",
